<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MappingRecipes" namespace="MappingRecipes.ManyToMany">
  <class name="Course">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <set name="Students" table="CourseStudent">
      <key column="CourseId"/>
      <many-to-many class="Student" column="StudentId"/>
    </set>
  </class>
</hibernate-mapping>